#songModal.reveal-modal.small(data-reveal='')
  .row
    .small-12.columns
      h3 Edit Song
  .row.collapse.padded-row
    .small-10.columns
      input(type='text', ng-model='editSong', placeholder='Name')
    .small-2.columns
      a.button.success.postfix(href='', ng-click='updateSong(song)') Update
  a.close-reveal-modal x
